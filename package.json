{
  "name": "chadlefort.com",
  "private": true,
  "description": "Personal website",
  "version": "1.0.0",
  "license": "0BSD",
  "repository": {
    "type": "git",
    "url": "https://github.com/ChadLefort/chadlefort.com.git"
  },
  "scripts": {
    "preinstall": "npm run type-check && npm run test",
    "build": "gatsby build",
    "develop": "gatsby develop -p 1337",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,md}\"",
    "start": "npm run develop",
    "serve": "gatsby serve",
    "clean": "gatsby clean",
    "lint": "eslint . --ext .ts,.tsx",
    "type-check": "tsc --pretty",
    "test": "jest"
  },
  "dependencies": {
    "@material-ui/core": "4.11.0",
    "@material-ui/lab": "^4.0.0-alpha.56",
    "gatsby": "^2.24.74",
    "gatsby-plugin-google-analytics": "^2.3.18",
    "gatsby-plugin-manifest": "^2.4.34",
    "gatsby-plugin-material-ui": "^2.1.10",
    "gatsby-plugin-offline": "^3.2.31",
    "gatsby-plugin-prefetch-google-fonts": "^1.4.3",
    "gatsby-plugin-react-helmet": "^3.3.14",
    "mdi-material-ui": "^6.19.0",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-helmet": "^6.1.0",
    "react-scroll": "^1.8.1"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^11.1.0",
    "@types/jest": "^26.0.14",
    "@types/node": "^14.11.8",
    "@types/react": "16.9.51",
    "@types/react-dom": "16.9.8",
    "@types/react-helmet": "^6.1.0",
    "@types/react-scroll": "^1.8.0",
    "@types/react-test-renderer": "^16.9.3",
    "@typescript-eslint/eslint-plugin": "^2.34.0",
    "@typescript-eslint/parser": "^2.34.0",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^26.5.2",
    "babel-preset-gatsby": "^0.5.14",
    "eslint": "^6.8.0",
    "eslint-config-react-app": "^5.2.1",
    "eslint-plugin-flowtype": "^4.7.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.3.1",
    "eslint-plugin-react": "^7.21.4",
    "eslint-plugin-react-hooks": "^2.5.1",
    "husky": "^4.3.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^26.5.3",
    "lint-staged": "^10.4.0",
    "prettier": "2.1.2",
    "react-test-renderer": "^16.14.0",
    "typescript": "4.0.3"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run type-check && lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.{ts,tsx}": [
      "eslint --fix",
      "prettier --write",
      "git add"
    ]
  }
}
